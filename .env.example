# Example environment variables for local development (non-sensitive defaults)
# Copy to .env and edit as needed. Do not commit your real secrets.

# Toggle universal handler as the default for unknown platforms
USE_UNIVERSAL_DEFAULT=false

# User/contact defaults used by handlers (universal/generic/rcm)
USER_EMAIL=test@example.com
USER_FIRST_NAME=Test
USER_LAST_NAME=User
USER_NAME=Test User
USER_COMPANY=Example Company LLC
USER_TITLE=Analyst
USER_PHONE=(555) 123-4567

# Optional: E2E deal room test URL (do NOT commit real URLs)
DEALROOM_URL=
# Direct SharePoint folder ID (preferred for uploads)
SHAREPOINT_FOLDER_ID=

# Optional alternates recognized by the universal handler
EMAIL=
PASSWORD=
USERNAME=
FULL_NAME=
FIRST_NAME=
LAST_NAME=
COMPANY=
TITLE=
PHONE=
WEBSITE=
ADDRESS1=
ADDRESS2=
CITY=
STATE=
POSTAL_CODE=
COUNTRY=

# JLL-specific (optional but recommended for JLL flows)
JLL_USERNAME=
JLL_EMAIL=
JLL_PASSWORD=
JLL_ESIGN_NAME=
JLL_TITLE=
JLL_COMPANY=

# Stagehand AI fallback (optional; disabled by default)
# Set STAGEHAND_GLOBAL_DISABLE=true to turn off all AI fallbacks immediately
STAGEHAND_GLOBAL_DISABLE=false
STAGEHAND_ENV=LOCAL           # or BROWSERBASE
STAGEHAND_VERBOSE=1           # 0|1|2
STAGEHAND_ENABLE_CACHE=true
# Provider API keys (only required when using fallback)
OPENAI_API_KEY=
ANTHROPIC_API_KEY=

# Test toggles
ENABLE_STAGEHAND_TESTS=false
